{
	"$schema": "https://biomejs.dev/schemas/2.1.4/schema.json",
	"extends": "//",
	"files": {
		"includes": ["**"]
	},
	"formatter": {
		"includes": ["**", "!**/*.test.ts", "!**/*.test.tsx"],
		"indentStyle": "tab"
	},
	"linter": {
		"includes": ["**", "!**/*.test.ts", "!**/*.test.tsx"],
		"rules": {
			"correctness": {
				"noUnusedFunctionParameters": "error"
			}
		}
	}
}
