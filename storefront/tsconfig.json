{
	"compilerOptions": {
		"paths": {
			"@/*": ["./app/*"]
		},
		"types": ["cypress", "@testing-library/cypress"],
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"moduleDetection": "force",
		"target": "es6",
		"jsx": "react-jsx",
		"strict": true,
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	}
}
